    "@types/webpack": "^4.4.0",
    "async-middleware": "^1.2.1",
    "awesome-typescript-loader": "^5.0.0",
    "axios": "^0.18.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.5",
